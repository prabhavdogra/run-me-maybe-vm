{
  "name": "cmm-assembly",
  "displayName": "cmm Assembly",
  "description": "Syntax highlighting for cmm Assembly Language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "cmm",
        "aliases": [
          "cmm Assembly",
          "cmm"
        ],
        "extensions": [
          ".cmm"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "cmm",
        "scopeName": "source.cmm",
        "path": "./syntaxes/cmm.tmLanguage.json"
      }
    ]
  }
}